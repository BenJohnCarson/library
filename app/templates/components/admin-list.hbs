<h1>{{type}}</h1>
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>ID</th>
      {{#each tableHeaders as |header|}}
        <th>{{header}}</th>
      {{/each}}
    </tr>
  </thead>
  <tbody>
  {{#each models as |model|}}
    {{admin-entry model=model}}
  {{/each}}
  </tbody>
</table>
