<h2>List</h2>
<div class="row">
  {{#each libraries as |library|}}
    {{#library-entry data-test-library=library.id library=library}}
      {{#link-to 'libraries.edit' library.id class='btn btn-success btn-xs'}}Edit{{/link-to}}
      <button class="btn btn-danger btn-xs" {{action 'deleteLibrary' library}} disabled={{deleteTask.isRunning}}>
        Delete
      </button>
    {{/library-entry}}
  {{/each}}
</div>
